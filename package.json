  "description": "An app that allows you to automatically record POV demos of all your CS:GO games",
  "main": "./.webpack/main",
  "version": "0.0.1",
  "scripts": {
    "start": "electron-forge start",
    "release": "yarn icons && electron-forge publish && node cleanup-release.js",
