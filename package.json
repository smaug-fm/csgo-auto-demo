    "make": "yarn icons && electron-forge make",
    "test": "webpack --config webpack/test.webpack.js && node .webpack/test/test.js",
    "icons": "node icon-other.js && icon-gen -i assets/app.svg -o ./assets --icns --icns-name app --ico --ico-name app",
    "lint": "eslint . --fix --ext '.js,.jsx,.ts,.tsx' && tsc",
    "prepare": "husky install",
    "pre-commit": "lint-staged && tsc"
  },
